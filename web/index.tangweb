<html>
<head>
<meta charset="UTF-8"> 
<link href="/index.css" rel="stylesheet" type="text/css">
<title>Page index test !</title>
<script>
var xhr = new XMLHttpRequest();
xhr.open("POST", "/", true);
xhr.setRequestHeader("Content-Type", "test");
xhr.onreadystatechange = function() {
    if (this.readyState == 4 && this.status == 200) {
        document.write(this.responseText);
        document.close();
    }
};
xhr.send("start if");
</script>
</head>
<body style="background: #222222;">

<export=noProcess>
<div class="processWindow">
    <span class="titleProcess">No process</span>
</div>
</export>

<export=oneProcess>
<div class="processWindowTitle"><span class="<import=etatProcess>"></span>
    <span class="titleProcess"><import=nameProcess></span>
    <form method="POST" action="/action_process" style="display: inline;"><input type="image" name="onStart <import=nameProcess>" value=" " class="<import=onStartProcess> right"></form>
    <form method="POST" action="/action_process" style="display: inline;"><input type="image" name="stop <import=nameProcess>" src="/stop.png" class="right"></form>
    <form method="POST" action="/action_process" style="display: inline;"><input type="image" name="restart <import=nameProcess>"  src="/restart.png" class="right"></form>
    <form method="POST" action="/action_process" style="display: inline;"><input type="image" name="start <import=nameProcess>"  src="/start.png" class="right"></form>
</div>
<div class="processWindow"><form method="POST" action="/action_process" style="display: inline;"><input type="image" name="cmd <import=nameProcess>" src="/cmd.png" class="labelProcess"></form> <span class="textProcess"><import=cmdProcess></span>
                        <br><form method="POST" action="/action_process" style="display: inline;"><input type="image" name="rep <import=nameProcess>" src="/rep.png" class="labelProcess"></form> <span class="textProcess"><import=repProcess></span>
</div>
<br>
</export>

</div>
</body>
</html>